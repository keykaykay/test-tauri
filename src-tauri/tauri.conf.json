{
  "build": {
    "beforeDevCommand": "yarn dev",
    "beforeBuildCommand": "yarn build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist"
  },
  "package": {
    "productName": "qizhidao",
    "version": "1.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": true,
      "shell": {
        "all": false,
        "open": true
      },
      "fs": {
        "all": true,
        "scope": ["$APP/**/*"]
      },
      "dialog": {
        "all": true
      }
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.zgtps.qzd",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "loading",
        "width": 1200,
        "height": 900,
        "visible": true,
        "label": "loading"
      },
      {
        "fullscreen": false,
        "resizable": true,
        "title": "企知道",
        "width": 1200,
        "height": 900,
        "url": "https://www.qizhidao.com/login",
        "visible": false,
        "label": "qizhidao"
      }
    ]
  }
}
